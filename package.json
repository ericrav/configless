{
  "name": "configless",
  "version": "0.1.2",
  "description": "Serverless plugin to configure functions using TypeScript decorators",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "yarn build",
    "precommit": "yarn test",
    "build": "rm -rf lib && rm -rf plugin && tsc -p tsconfig.build.json && mv lib/plugin plugin",
    "pretest": "yarn lint",
    "test": "jest",
    "lint": "eslint -c .eslintrc.js --ext .ts src"
  },
  "author": "Eric Rabinowitz <er@ericrbainowitz.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericrav/configless.git"
  },
  "keywords": [
    "serverless",
    "1.0",
    "typescript",
    "plugin",
    "decorators",
    "aws lambda"
  ],
  "files": [
    "lib",
    "plugin"
  ],
  "devDependencies": {
    "@types/fs-extra": "^5.0.5",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.0",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.5.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.16.0",
    "jest": "^24.7.1",
    "serverless": "^1.40.0",
    "ts-jest": "^24.0.1"
  },
  "dependencies": {
    "@types/serverless": "^1.18.2",
    "aws-sdk": "^2.433.0",
    "deepmerge": "^3.2.0",
    "fs-extra": "^7.0.1",
    "typescript": "^3.4.1"
  }
}
